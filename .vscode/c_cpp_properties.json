{
    "env": {
        "myIncludePath": [
            "${workspaceFolder}/include",
            "/usr/include/x86_64-linux-musl"
        ],
        "myDefines": [
            "DEBUG",
            "MY_FEATURE=1",
            "_POSIX_C_SOURCE=200809L",
            "_XOPEN_SOURCE=800",
            "PLATFORM_LINUX",
            "MUSL",
            "NDEBUG"
        ]
    },
    "configurations": [
        {
            "name": "Linux AMD64 (musl-gcc)",
            "compilerPath": "/usr/bin/musl-gcc",
            "compilerArgs": [
            "-std=c23",
            "-D_POSIX_C_SOURCE=200809L",
            "-D_XOPEN_SOURCE=800",
            "-O3",
            "-Wall",
            "-Wextra",
            "-Werror",
            "-pedantic",
            "-I/usr/include/x86_64-linux-musl",
            "-I${workspaceFolder}/include",
            "-L/usr/lib/x86_64-linux-musl",
            "-static",
            "-fno-common",
            "-ffunction-sections",
            "-fdata-sections",
            "-march=x86-64",
            "-Wshadow",
            "-Wconversion",
            "-Wstrict-prototypes",
            "-Wmissing-prototypes",
            "-Wold-style-definition",
            "-Wmissing-declarations",
            "-Wredundant-decls",
            "-Wfloat-equal",
            "-Wundef",
            "-Wbad-function-cast",
            "-Wcast-qual",
            "-Wwrite-strings",
            "-Wformat=2",
            "-Wstrict-aliasing=2",
            "-Wstrict-overflow=5",
            "-Wnull-dereference",
            "-Wdouble-promotion",
            "-Wvla",
            "-Wcast-align",
            "-Wpointer-arith",
            "-Wswitch-default",
            "-Wunreachable-code"
            ],
            "intelliSenseMode": "linux-gcc-x64",
            "includePath": [
                "${myIncludePath}"
            ],
            "defines": [
                "${myDefines}"
            ],
            "cStandard": "c23",
            "browse": {
                "path": [
                    "${myIncludePath}",
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": "${workspaceFolder}/.vscode/browse.vc.db"
            }
        }
    ],
    "version": 4,
    "enableConfigurationSquiggles": true
}
