{
    "env": {
        "myIncludePath": [
            "${workspaceFolder}/include",
            "${workspaceFolder}/lib",
            "/usr/include"
        ],
        "myDefines": [
            "DEBUG",
            "MY_FEATURE=1",
            "_POSIX_C_SOURCE=200809L",
            "_XOPEN_SOURCE=700",
            "PLATFORM_LINUX",
            "TINYCC",
            "MUSL",
            "NDEBUG"
        ]
    },
    "configurations": [
        {
            "name": "Linux AMD64 (musl)",
            "compilerPath": "/usr/bin/tcc",
            "compilerArgs": [
                "-std=c11",
                "-D_POSIX_C_SOURCE=200809L",
                "-D_XOPEN_SOURCE=700",
                "-I/usr/include",
                "-I${workspaceFolder}/include",
                "-L/usr/lib",
                "-static",
                "-fno-common",
                "-Wall",
                "-Werror",
                "-g",
                "-ffunction-sections",
                "-fdata-sections",
                "-march=x86-64"
            ],
            "intelliSenseMode": "linux-gcc-x64",
            "includePath": [
                "${myIncludePath}"
            ],
            "defines": [
                "${myDefines}"
            ],
            "cStandard": "c11",
            "browse": {
                "path": [
                    "${myIncludePath}",
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": "${workspaceFolder}/.vscode/browse.vc.db"
            }
        }
    ],
    "version": 4,
    "enableConfigurationSquiggles": true
}
