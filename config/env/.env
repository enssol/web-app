# .env
APP_NAME=enssol
APP_VERSION=1.0.0
APP_ENV=development
APP_DEBUG=true
APP_PORT=8080
APP_HOST=localhost

# Logging configuration
LOG_LEVEL=debug
LOG_PATH=/var/log/app.log
LOG_FORMAT=text
LOG_MAX_SIZE=10485760

# Database configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=webapp
DB_USER=postgres
DB_PASSWORD=

# Cache configuration
CACHE_DRIVER=file
CACHE_PREFIX=webapp_cache_
CACHE_TTL=3600
